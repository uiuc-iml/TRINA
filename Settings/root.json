{
    "robot": "@robot_bubonic.json",
    "user": {
        "configs":"@anthrax/configs.json"
    },
    "MotionServer": {
        "mode": "Kinematic",
        "ip_address":"127.0.0.1",
        "port":8080,
        "components":["left_limb","right_limb","base"]
    },
    "RedisServer": {
        "ip_address":"localhost"
    },
    "CommandServer":{
        "time_updater_dt":0.001,
        "command_receiver_dt":0.001,
        "health_monitor_dt":1,
        "heartbeat_tolerance":10,
        "#_comment1":"If you would like to access a different class in your module rather than ModuleName, put the class name here",
        "module_class_map": {
            "Motion":"MotionModule",
            "UI":"LocalUI"
        },
        "verbose":0,
        "#_comment2":"To force modules to be verbose, put them here",
        "debug":[]
    },
    "Simulation":{
        "world_file":"Models/simulation_worlds/TRINA_world_bubonic_with_obstacle.xml"
    },
    "DebugUI": {
        "world_file":"Models/simulation_worlds/TRINA_world_bubonic_with_obstacle.xml"
    },
    "MotionModule": {
        "state_receiver_dt":0.015
    },
    "CalibrationModule" : {
        "right_calibration_configs": "calibration/right_sensor_calibration.configs",
        "left_calibration_configs": "calibration/right_sensor_calibration.configs",
        "fixed_calibration_configs": "calibration/fixed_sensor_calibration.configs"
    }
    
}